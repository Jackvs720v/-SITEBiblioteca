{"ast":null,"code":"import { Chart } from \"chart.js/auto\";\nexport default {\n  methods: {\n    generatePDF() {\n      alert(\"PDF Gerado!\");\n    },\n    createCharts() {\n      // Gráfico de pizza para \"Livros mais Emprestados\"\n      new Chart(document.getElementById(\"pieChart\").getContext(\"2d\"), {\n        type: \"pie\",\n        data: {\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\n          datasets: [{\n            data: [33.33, 33.33, 33.33],\n            backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#ffcd56\"]\n          }]\n        }\n      });\n\n      // Gráfico de barras para \"Histórico de Reservas\"\n      new Chart(document.getElementById(\"barChart\").getContext(\"2d\"), {\n        type: \"bar\",\n        data: {\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\n          datasets: [{\n            data: [50, 70, 40],\n            backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#4bc0c0\"]\n          }]\n        }\n      });\n\n      // Gráfico de pizza para \"Usuários mais Ativos\"\n      new Chart(document.getElementById(\"pieChartUsers\").getContext(\"2d\"), {\n        type: \"pie\",\n        data: {\n          labels: [\"Cleiton1974\", \"Eduardo777\", \"Seila674\"],\n          datasets: [{\n            data: [40, 35, 25],\n            backgroundColor: [\"#36a2eb\", \"#ff6384\", \"#4bc0c0\"]\n          }]\n        }\n      });\n    }\n  },\n  mounted() {\n    this.createCharts();\n  }\n};","map":{"version":3,"names":["Chart","methods","generatePDF","alert","createCharts","document","getElementById","getContext","type","data","labels","datasets","backgroundColor","mounted"],"sources":["C:\\Users\\PC\\Pictures\\-SITEBiblioteca\\frontend\\loginfrontend\\src\\views\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-app\">\r\n    <!-- Navbar -->\r\n    <nav class=\"navbar\">\r\n      <div class=\"navbar-left\">\r\n        <img src=\"@/assets/logo.png\" alt=\"Logo\" class=\"navbar-logo\" />\r\n      </div>\r\n      <div class=\"navbar-center\">\r\n        <div class=\"search-container\">\r\n          <img src=\"@/assets/loupe.png\" alt=\"Buscar\" class=\"search-icon\" />\r\n          <input\r\n            type=\"text\"\r\n            class=\"navbar-search\"\r\n            placeholder=\"Pesquisar livros...\"\r\n          />\r\n          <img src=\"@/assets/filter.png\" alt=\"Filtrar\" class=\"search-icon\" />\r\n        </div>\r\n      </div>\r\n      <div class=\"navbar-right\">\r\n        <a href=\"\">\r\n          <img src=\"@/assets/crud.png\" alt=\"Crud\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/dashboard.png\" alt=\"Dashboard\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/salvar.png\" alt=\"Salvar\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/notificacao.png\" alt=\"Notificação\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/perfil.png\" alt=\"Perfil\" class=\"icon\" />\r\n        </a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Dashboard Content -->\r\n    <div class=\"dashboard\">\r\n      <div class=\"content\">\r\n        <div class=\"card\">\r\n          <h2>Livros mais Emprestados</h2>\r\n          <canvas id=\"pieChart\"></canvas>\r\n        </div>\r\n        <div class=\"card\">\r\n          <h2>Usuários mais Ativos</h2>\r\n          <canvas id=\"pieChartUsers\"></canvas>\r\n        </div>\r\n        <div class=\"card\">\r\n          <h2>Histórico de Reservas</h2>\r\n          <canvas id=\"barChart\"></canvas>\r\n        </div>\r\n        <div class=\"card pdf-button\">\r\n          <button @click=\"generatePDF\">Gerar PDF</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Chart } from \"chart.js/auto\";\r\n\r\nexport default {\r\n  methods: {\r\n    generatePDF() {\r\n      alert(\"PDF Gerado!\");\r\n    },\r\n    createCharts() {\r\n      // Gráfico de pizza para \"Livros mais Emprestados\"\r\n      new Chart(document.getElementById(\"pieChart\").getContext(\"2d\"), {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\r\n          datasets: [\r\n            {\r\n              data: [33.33, 33.33, 33.33],\r\n              backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#ffcd56\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n\r\n      // Gráfico de barras para \"Histórico de Reservas\"\r\n      new Chart(document.getElementById(\"barChart\").getContext(\"2d\"), {\r\n        type: \"bar\",\r\n        data: {\r\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\r\n          datasets: [\r\n            {\r\n              data: [50, 70, 40],\r\n              backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#4bc0c0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n\r\n      // Gráfico de pizza para \"Usuários mais Ativos\"\r\n      new Chart(document.getElementById(\"pieChartUsers\").getContext(\"2d\"), {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: [\"Cleiton1974\", \"Eduardo777\", \"Seila674\"],\r\n          datasets: [\r\n            {\r\n              data: [40, 35, 25],\r\n              backgroundColor: [\"#36a2eb\", \"#ff6384\", \"#4bc0c0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.createCharts();\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.dashboard-app {\r\n  font-family: \"Arial\", sans-serif;\r\n  background-color: #133957; /* Fundo azul escuro */\r\n  color: #fff; /* Cor do texto padrão */\r\n  height: 100vh; /* Ocupar toda a altura da janela */\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  background-color: #bcd7e5;\r\n  padding: 20px 40px; /* Aumentar o espaçamento interno */\r\n  border-bottom: 2px solid #007bff;\r\n  height: 80px; /* Aumentar a altura da barra */\r\n}\r\n.navbar-left {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.navbar-logo {\r\n  height: 60px; /* Ajustar tamanho do logo */\r\n}\r\n.navbar-center {\r\n  flex: 1;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  border: 1px solid #ddd;\r\n  border-radius: 12px;\r\n  padding: 10px 20px; /* Aumentar o padding */\r\n  width: 500px; /* Tornar o campo de busca maior */\r\n}\r\n.navbar-search {\r\n  flex: 1;\r\n  border: none;\r\n  outline: none;\r\n  font-size: 1.1em; /* Texto maior */\r\n}\r\n.search-icon {\r\n  width: 24px;\r\n  height: 24px; /* Ícones maiores */\r\n  margin: 0 10px;\r\n}\r\n.navbar-right .icon {\r\n  width: 40px;\r\n  height: 40px; /* Ícones do lado direito maiores */\r\n  margin-left: 20px;\r\n}\r\n\r\n/* Dashboard */\r\n.dashboard {\r\n  padding: 30px;\r\n}\r\n.content {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 30px; /* Maior espaçamento entre os cards */\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n.card {\r\n  background-color: #f9f9f9;\r\n  border-radius: 16px;\r\n  padding: 20px; /* Mais espaçamento interno */\r\n  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\r\n  text-align: center;\r\n  color: #000;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  height: 400px; /* Aumentar a altura dos cards */\r\n}\r\n.card h2 {\r\n  font-size: 1.4em; /* Texto maior */\r\n  margin-bottom: 20px;\r\n}\r\ncanvas {\r\n  margin: auto;\r\n  max-width: 95%; /* Ajustar tamanho do gráfico */\r\n  max-height: 80%; /* Permitir gráficos maiores */\r\n}\r\n.pdf-button {\r\n  grid-column: span 2;\r\n  text-align: center;\r\n}\r\n.pdf-button button {\r\n  background-color: #dc3545;\r\n  color: white;\r\n  padding: 15px 30px; /* Botão maior */\r\n  border: none;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  font-size: 18px; /* Texto maior */\r\n}\r\n.pdf-button button:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n/* Ajustes gerais */\r\n.content {\r\n  width: 90%;\r\n  max-width: 1400px; /* Permitir que os cards ocupem mais espaço */\r\n  margin: 0 auto;\r\n}\r\n</style>\r\n\r\n"],"mappings":"AA4DA,SAASA,KAAI,QAAS,eAAe;AAErC,eAAe;EACbC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,KAAK,CAAC,aAAa,CAAC;IACtB,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb;MACA,IAAIJ,KAAK,CAACK,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC,EAAE;QAC9DC,IAAI,EAAE,KAAK;QACXC,IAAI,EAAE;UACJC,MAAM,EAAE,CAAC,qBAAqB,EAAE,qBAAqB,EAAE,YAAY,CAAC;UACpEC,QAAQ,EAAE,CACR;YACEF,IAAI,EAAE,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;YAC3BG,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS;UACnD,CAAC;QAEL;MACF,CAAC,CAAC;;MAEF;MACA,IAAIZ,KAAK,CAACK,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC,EAAE;QAC9DC,IAAI,EAAE,KAAK;QACXC,IAAI,EAAE;UACJC,MAAM,EAAE,CAAC,qBAAqB,EAAE,qBAAqB,EAAE,YAAY,CAAC;UACpEC,QAAQ,EAAE,CACR;YACEF,IAAI,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC;YAClBG,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS;UACnD,CAAC;QAEL;MACF,CAAC,CAAC;;MAEF;MACA,IAAIZ,KAAK,CAACK,QAAQ,CAACC,cAAc,CAAC,eAAe,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC,EAAE;QACnEC,IAAI,EAAE,KAAK;QACXC,IAAI,EAAE;UACJC,MAAM,EAAE,CAAC,aAAa,EAAE,YAAY,EAAE,UAAU,CAAC;UACjDC,QAAQ,EAAE,CACR;YACEF,IAAI,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC;YAClBG,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS;UACnD,CAAC;QAEL;MACF,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACT,YAAY,CAAC,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}