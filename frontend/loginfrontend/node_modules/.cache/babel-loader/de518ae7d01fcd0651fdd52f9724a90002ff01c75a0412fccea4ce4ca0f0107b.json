al após o cadastro\r\n      } catch (error) {\r\n        this.message = error.response && error.response.data && error.response.data.message\r\n          ? error.response.data.message\r\n          : 'Erro ao fazer cadastro.';\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  min-height: 100vh;\r\n  background-color: #1c3b4c;\r\n  font-family: 'Arial', sans-serif;\r\n  padding: 10px;\r\n}\r\n\r\n.register-header {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.logo {\r\n  width: 50%;\r\n  margin-bottom: 10%;\r\n}\r\n\r\n\r\nh1 {\r\n  color: white;\r\n  font-size: 24px;\r\n  margin: 0;\r\n}\r\n\r\nform {\r\n  background-color: #2a4d61;\r\n  padding: 20px 30px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  width: 100%;\r\n  max-width: 350px;\r\n  text-align: center;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n  text-align: left;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  color: white;\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  margin-bottom: 10px;\r\n  transition: box-shadow 0.3s;\r\n  font-size: 14px;\r\n}\r\n\r\ninput:focus {\r\n  box-shadow: 0 0 5px 2px #007bff;\r\n  outline: none;\r\n}\r\n\r\nbutton {\r\n  padding: 12px;\r\n  background-color: #ffffff;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  width: 48%; /* Ajusta para manter uma boa largura entre os botões */\r\n  transition: background-color 0.3s, transform 0.3s;\r\n  color: #1c3b4c;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #f0f0f0;\r\n}\r\n\r\np {\r\n  margin-top: 15px;\r\n  color: #e74c3c;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":";AAyCA,OAAOA,GAAE,MAAO,UAAU,EAAE;;AAE5B,eAAe;EACbC,IAAIA,CAAA,EAAG;IAC