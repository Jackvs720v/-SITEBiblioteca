{"ast":null,"code":"import Navbar from \"../\"; // Importando o componente Navbar\n\nexport default {\n  data() {\n    return {\n      searchQuery: \"\",\n      books: [{\n        id: 1,\n        title: \"Vida Fora Da Gangue\",\n        author: \"Pedro Stiehl\",\n        rating: \"⭐⭐⭐⭐⭐\",\n        pages: 130,\n        isbn: \"8587187449\",\n        copies: 500,\n        available: 498,\n        synopsis: \"Quatro amigos e o jornal da escola investigam a misteriosa inutilização de livros da biblioteca.\",\n        publicationYear: 2000,\n        cover: \"@/assets/cover1.jpg\",\n        comments: [{\n          id: 1,\n          user: \"Seila1674\",\n          text: \"É um excelente livro, simplesmente incrível !!!\"\n        }, {\n          id: 2,\n          user: \"Cleiton1974\",\n          text: \"Simplesmente... Cinema\"\n        }]\n      }\n      // Adicione mais livros aqui\n      ]\n    };\n  },\n  components: {\n    Navbar // Registrando o componente\n  },\n  computed: {\n    filteredBooks() {\n      return this.books.filter(book => book.title.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    onSearch() {\n      console.log(\"Busca: \", this.searchQuery);\n    },\n    reserveBook(book) {\n      console.log(\"Reservando o livro: \" + book.title);\n    }\n  }\n};","map":{"version":3,"names":["Navbar","data","searchQuery","books","id","title","author","rating","pages","isbn","copies","available","synopsis","publicationYear","cover","comments","user","text","components","computed","filteredBooks","filter","book","toLowerCase","includes","methods","onSearch","console","log","reserveBook"],"sources":["C:\\Users\\PC\\Pictures\\-SITEBiblioteca\\frontend\\loginfrontend\\src\\views\\detalhesBook.vue"],"sourcesContent":["<template>\r\n  <div id=\"detalhesBook\">\r\n    <!-- Navbar -->\r\n\r\n    <!-- Main Content - Cartões de livros -->\r\n    <div class=\"layout\">\r\n      <div class=\"book-container\">\r\n        <div class=\"book-card\" v-for=\"book in filteredBooks\" :key=\"book.id\">\r\n          <div class=\"book-header\">\r\n            <img :src=\"book.cover\" alt=\"Capa do livro\" class=\"book-cover\" />\r\n            <div class=\"book-info\">\r\n              <h3 class=\"book-title\">{{ book.title }}</h3>\r\n              <p class=\"book-author\">Autor: {{ book.author }}</p>\r\n              <p class=\"book-pages\">Páginas: {{ book.pages }}</p>\r\n              <p class=\"book-isbn\">ISBN: {{ book.isbn }}</p>\r\n              <p class=\"book-copies\">Cópias: {{ book.copies }}</p>\r\n              <p class=\"book-available\">Cópias disponíveis: {{ book.available }}</p>\r\n              <p class=\"book-synopsis\">{{ book.synopsis }}</p>\r\n              <p class=\"book-publication\">Publicação: {{ book.publicationYear }}</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"book-footer\">\r\n            <div class=\"book-rating\">Avaliação: {{ book.rating }}</div>\r\n            <div class=\"book-comments\">\r\n              <p>Comentários ({{ book.comments.length }})</p>\r\n              <ul>\r\n                <li v-for=\"comment in book.comments\" :key=\"comment.id\">\r\n                  <strong>{{ comment.user }}:</strong> {{ comment.text }}\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <button @click=\"reserveBook(book)\">Reservar</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n    import Navbar from \"../\"; // Importando o componente Navbar\r\n    \r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: \"\",\r\n      books: [\r\n        {\r\n          id: 1,\r\n          title: \"Vida Fora Da Gangue\",\r\n          author: \"Pedro Stiehl\",\r\n          rating: \"⭐⭐⭐⭐⭐\",\r\n          pages: 130,\r\n          isbn: \"8587187449\",\r\n          copies: 500,\r\n          available: 498,\r\n          synopsis:\r\n            \"Quatro amigos e o jornal da escola investigam a misteriosa inutilização de livros da biblioteca.\",\r\n          publicationYear: 2000,\r\n          cover: \"@/assets/cover1.jpg\",\r\n          comments: [\r\n            { id: 1, user: \"Seila1674\", text: \"É um excelente livro, simplesmente incrível !!!\" },\r\n            { id: 2, user: \"Cleiton1974\", text: \"Simplesmente... Cinema\" },\r\n          ],\r\n        },\r\n        // Adicione mais livros aqui\r\n      ],\r\n    };\r\n  },\r\n  components: {\r\n    Navbar, // Registrando o componente\r\n  },\r\n  computed: {\r\n    filteredBooks() {\r\n      return this.books.filter((book) =>\r\n        book.title.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    onSearch() {\r\n      console.log(\"Busca: \", this.searchQuery);\r\n    },\r\n    reserveBook(book) {\r\n      console.log(\"Reservando o livro: \" + book.title);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Estilos gerais para o layout */\r\n#detalhesBook {\r\n  font-family: 'Arial', sans-serif;\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px 20px;\r\n  background-color: #007bff;\r\n  color: white;\r\n}\r\n\r\n.navbar-left, .navbar-center, .navbar-right {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.navbar-logo {\r\n  height: 40px;\r\n}\r\n\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  border-radius: 20px;\r\n  padding: 5px 15px;\r\n  margin: 0 20px;\r\n}\r\n\r\n.navbar-search {\r\n  border: none;\r\n  outline: none;\r\n  font-size: 16px;\r\n  padding: 5px;\r\n}\r\n\r\n.search-icon {\r\n  width: 20px;\r\n  height: 20px;\r\n  margin-right: 10px;\r\n}\r\n\r\n/* Cartões de livro */\r\n.layout {\r\n  padding: 20px;\r\n}\r\n\r\n.book-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n\r\n.book-card {\r\n  width: 300px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  margin: 10px;\r\n  padding: 15px;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.book-header {\r\n  display: flex;\r\n}\r\n\r\n.book-cover {\r\n  width: 80px;\r\n  height: 120px;\r\n  margin-right: 15px;\r\n}\r\n\r\n.book-info {\r\n  flex: 1;\r\n}\r\n\r\n.book-title {\r\n  font-size: 1.6em;\r\n  font-weight: bold;\r\n}\r\n\r\n.book-author,\r\n.book-pages,\r\n.book-isbn,\r\n.book-copies,\r\n.book-available,\r\n.book-synopsis,\r\n.book-publication {\r\n  font-size: 1em;\r\n  margin: 5px 0;\r\n}\r\n\r\n.book-footer {\r\n  margin-top: 15px;\r\n  text-align: center;\r\n}\r\n\r\n.book-rating {\r\n  font-size: 1.2em;\r\n}\r\n\r\n.book-comments {\r\n  font-size: 1em;\r\n  margin-top: 10px;\r\n}\r\n\r\n.book-comments ul {\r\n  list-style: none;\r\n  padding: 0;\r\n}\r\n\r\n.book-comments li {\r\n  margin: 5px 0;\r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":"AAwCI,OAAOA,MAAK,MAAO,KAAK,EAAE;;AAG9B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,CACL;QACEC,EAAE,EAAE,CAAC;QACLC,KAAK,EAAE,qBAAqB;QAC5BC,MAAM,EAAE,cAAc;QACtBC,MAAM,EAAE,OAAO;QACfC,KAAK,EAAE,GAAG;QACVC,IAAI,EAAE,YAAY;QAClBC,MAAM,EAAE,GAAG;QACXC,SAAS,EAAE,GAAG;QACdC,QAAQ,EACN,kGAAkG;QACpGC,eAAe,EAAE,IAAI;QACrBC,KAAK,EAAE,qBAAqB;QAC5BC,QAAQ,EAAE,CACR;UAAEX,EAAE,EAAE,CAAC;UAAEY,IAAI,EAAE,WAAW;UAAEC,IAAI,EAAE;QAAkD,CAAC,EACrF;UAAEb,EAAE,EAAE,CAAC;UAAEY,IAAI,EAAE,aAAa;UAAEC,IAAI,EAAE;QAAyB,CAAC;MAElE;MACA;MAAA;IAEJ,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVlB,MAAM,CAAE;EACV,CAAC;EACDmB,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACjB,KAAK,CAACkB,MAAM,CAAEC,IAAI,IAC5BA,IAAI,CAACjB,KAAK,CAACkB,WAAW,CA