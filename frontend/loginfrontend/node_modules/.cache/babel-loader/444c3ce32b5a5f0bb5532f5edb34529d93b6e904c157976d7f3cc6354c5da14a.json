ound-color: #bcd7e5;\r\n  padding: 15px 30px;\r\n  border-bottom: 2px solid #007bff;\r\n}\r\n.navbar-left {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.navbar-logo {\r\n  height: 80px;\r\n  margin-right: 10px;\r\n}\r\n.navbar-center {\r\n  display: flex;\r\n  align-items: center;\r\n  flex: 1;\r\n  justify-content: center;\r\n}\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  border: 1px solid #ddd;\r\n  border-radius: 12px;\r\n  padding: 5px 15px;\r\n  width: 600px;\r\n}\r\n.navbar-search {\r\n  flex: 1;\r\n  border: none;\r\n  outline: none;\r\n  font-size: 1.2em;\r\n  padding: 8px;\r\n}\r\n.search-icon {\r\n  width: 30px;\r\n  height: 30px;\r\n  margin: 0 10px;\r\n}\r\n/* Ajuste para os ícones da navbar */\r\n.navbar-right .icon {\r\n  width: 30px; /* Ajuste para um tamanho menor */\r\n  height: 30px;\r\n  color: #007bff;\r\n  margin-left: 15px;\r\n  cursor: pointer;\r\n  transition: color 0.3s;\r\n}\r\n.navbar-right .icon:hover {\r\n  color: #0056b3;\r\n}\r\n\r\n/* Ajuste para os ícones da barra de pesquisa */\r\n.navbar-center .search-icon {\r\n  width: 25px; /* Dimensão menor para os ícones de busca e filtro */\r\n  height: 25px;\r\n  margin: 0 10px;\r\n}\r\n\r\n.right-icon {\r\n  width: 5%; /* Reduzido para 25px de largura */\r\n  height: 5%;/* Reduzido para 25px de altura */\r\n  margin-left: 15px;\r\n}\r\n\r\n\r\n  </style>\r\n    "],"mappings":"AAyCE,eAAe;EACbA,IAAI,EAAE,QAAQ;EACdC;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}