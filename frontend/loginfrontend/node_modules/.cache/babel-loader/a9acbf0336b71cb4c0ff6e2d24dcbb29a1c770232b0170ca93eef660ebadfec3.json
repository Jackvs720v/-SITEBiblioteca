\n          <img src=\"@/assets/loupe.png\" alt=\"Buscar\" class=\"search-icon\" />\r\n          <input\r\n            type=\"text\"\r\n            class=\"navbar-search\"\r\n            placeholder=\"Pesquisar livros...\"\r\n          />\r\n          <img src=\"@/assets/filter.png\" alt=\"Filtrar\" class=\"search-icon\" />\r\n        </div>\r\n      </div>\r\n      <div class=\"navbar-right\">\r\n        <a href=\"\">\r\n          <img src=\"@/assets/crud.png\" alt=\"Crud\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/dashboard.png\" alt=\"Dashboard\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/salvar.png\" alt=\"Salvar\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/notificacao.png\" alt=\"Notificação\" class=\"icon\" />\r\n        </a>\r\n        <a href=\"\">\r\n          <img src=\"@/assets/perfil.png\" alt=\"Perfil\" class=\"icon\" />\r\n        </a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Dashboard Content -->\r\n    <div class=\"dashboard\">\r\n      <div class=\"content\">\r\n        <div class=\"card\">\r\n          <h2>Livros mais Emprestados</h2>\r\n          <canvas id=\"pieChart\"></canvas>\r\n        </div>\r\n        <div class=\"card\">\r\n          <h2>Usuários mais Ativos</h2>\r\n          <canvas id=\"pieChartUsers\"></canvas>\r\n        </div>\r\n        <div class=\"card\">\r\n          <h2>Histórico de Reservas</h2>\r\n          <canvas id=\"barChart\"></canvas>\r\n        </div>\r\n        <div class=\"card pdf-button\">\r\n          <button @click=\"generatePDF\">Gerar PDF</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Chart } from \"chart.js/auto\";\r\n\r\nexport default {\r\n  methods: {\r\n    generatePDF() {\r\n      alert(\"PDF Gerado!\");\r\n    },\r\n    createCharts() {\r\n      // Gráfico de pizza para \"Livros mais Emprestados\"\r\n      new Chart(document.getElementById(\"pieChart\").getContext(\"2d\"), {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\r\n          datasets: [\r\n            {\r\n              data: [33.33, 33.33, 33.33],\r\n              backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#ffcd56\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n\r\n      // Gráfico de barras para \"Histórico de Reservas\"\r\n      new Chart(document.getElementById(\"barChart\").getContext(\"2d\"), {\r\n        type: \"bar\",\r\n        data: {\r\n          labels: [\"Vida Fora da Gangue\", \"O Menino Maluquinho\", \"O Príncipe\"],\r\n          datasets: [\r\n            {\r\n              data: [50, 70, 40],\r\n              backgroundColor: [\"#ff6384\", \"#36a2eb\", \"#4bc0c0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n\r\n      // Gráfico de pizza para \"Usuários mais Ativos\"\r\n      new Chart(document.getElementById(\"pieChartUsers\").getContext(\"2d\"), {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: [\"Cleiton1974\", \"Eduardo777\", \"Seila674\"],\r\n          datasets: [\r\n            {\r\n              data: [40, 35, 25],\r\n              backgroundColor: [\"#36a2eb\", \"#ff6384\", \"#4bc0c0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.createCharts();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard-app {\r\n  font-family: \"Arial\", sans-serif;\r\n  background-color: #133957; /* Fundo azul escuro */\r\n  color: #fff; /* Cor do texto padrão */\r\n  height: 100vh; /* Garantir que ocupe toda a tela */\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  background-color: #bcd7e5;\r\n  padding: 15px 30px;\r\n  border-bottom: 2px solid #007bff;\r\n  height: 70px; /* Altura reduzida */\r\n}\r\n.navbar-left {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.navbar-logo {\r\n  height: 50px; /* Tamanho ajustado */\r\n}\r\n.navbar-center {\r\n  flex: 1;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  border: 1px solid #ddd;\r\n  border-radius: 12px;\r\n  padding: 5px 15px;\r\n  width: 400px; /* Campo de busca */\r\n}\r\n.navbar-search {\r\n  flex: 1;\r\n  border: none;\r\n  outline: none;\r\n  font-size: 1em;\r\n}\r\n.search-icon {\r\n  width: 20px;\r\n  height: 20px;\r\n  margin: 0 10px;\r\n}\r\n.navbar-right .icon {\r\n  width: 30px;\r\n  height: 30px;\r\n  margin-left: 15px;\r\n}\r\n\r\n/* Dashboard */\r\n.dashboard {\r\n  flex: 1;\r\n  display: grid;\r\n  grid-template-areas:\r\n    \"livros usuarios\"\r\n    \"reservas pdf\";\r\n  grid-template-columns: 2fr 1.5fr; /* Proporção largura */\r\n  grid-template-rows: 1fr 1fr; /* Dividir espaço */\r\n  gap: 15px; /* Espaçamento entre os cards */\r\n  padding: 15px;\r\n  height: calc(100% - 70px); /* Ajustar altura do conteúdo */\r\n  box-sizing: border-box;\r\n}\r\n.card {\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  padding: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  text-align: center;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n.card h2 {\r\n  font-size: 1.2em; /* Tamanho do texto */\r\n  margin-bottom: 10px;\r\n}\r\ncanvas {\r\n  margin: auto;\r\n  max-width: 100%;\r\n  max-height: 75%; /* Ajustar o gráfico ao card */\r\n}\r\n\r\n/* Especificação dos cards */\r\n.card:nth-child(1) {\r\n  grid-area: livros; /* Livros mais emprestados */\r\n}\r\n.card:nth-child(2) {\r\n  grid-area: usuarios; /* Usuários mais ativos */\r\n}\r\n.card:nth-child(3) {\r\n  grid-area: reservas; /* Histórico de reservas */\r\n}\r\n.card:nth-child(4) {\r\n  grid-area: pdf; /* Botão PDF */\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n.pdf-button button {\r\n  background-color: #dc3545;\r\n  color: white;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n}\r\n.pdf-button button:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n/* Ajustar o tamanho para caber na tela */\r\n.content {\r\n  height: 100%;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n}\r\n</style>\r\n"],"mappings":";OAKaA,UAAuB;OAIrBC,UAAwB;OAMxBC,UAAyB;OAKzBC,UAAuB;OAGvBC,UAA4B;OAG5BC,UAAyB;OAGzBC,UAA8B;OAG9BC,UAAyB;;EA/BjCC,KAAK,EAAC;AAAe;;EAqCnBA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAS;;EAabA,KAAK,EAAC;AAAiB;;uBAnDlCC,mBAAA,CAwDM,OAxDNC,UAwDM,GAvDJC,mBAAA,YAAe,E,0BAFnBC,kBAAA,kGAAAZ,UAAA,4KAAAC,UAAA,4KAAAC,UAAA,iKAAAC,UAAA,mGAAAC,UAAA,wGAAAC,UAAA,qGAAAC,UAAA,0GAAAC,UAAA,6EAqCII,mBAAA,uBAA0B,EAC1BE,mBAAA,CAkBM,OAlBNC,UAkBM,GAjBJD,mBAAA,CAgBM,OAhBNE,UAgBM,G,0BAvDZH,kBAAA,2aAoDQC,mBAAA,CAEM,OAFNG,UAEM,GADJH,mBAAA,CAA+C;IAAtCI,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;KAAE,WAAS,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}